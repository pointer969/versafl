        xmlns:mvc="sap.ui.core.mvc"
        xmlns:f="sap.f"
        xmlns:w="sap.ui.integration.widgets">
    <f:GridContainer class="sapUiSmallMargin">
        <w:Card manifest="./test-resources/sap/ui/integration/demokit/sample/TimelineCard/model/TeamMembersManifest.json">
            <w:layoutData>
                <f:GridContainerItemLayoutData columns="4" />
            </w:layoutData>
        </w:Card>
        <w:Card manifest="./test-resources/sap/ui/integration/demokit/sample/TimelineCard/model/ActivitiesManifest.json">
            <w:layoutData>
