<mvc:View
		controllerName="sap.f.sample.ShellBarIconMenu.ShellBarIconMenu"
		xmlns:mvc="sap.ui.mvc"
		xmlns="sap.f"
		xmlns:m="sap.m"
		height="100%">
	<ShellBar
			title="Application Title"
			secondTitle="Short description"
			homeIcon="./resources/sap/ui/documentation/sdk/images/logo_ui5.png"
			showCopilot="true"
			showSearch="true"
			showNavButton="true"
			showNotifications="true"
			showProductSwitcher="true"
			notificationsNumber="2">
			<profile>
				<Avatar initials="UI"/>
			</profile>
	</ShellBar>
</mvc:View>